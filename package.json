{
  "name": "koa-redirects",
  "version": "1.0.0",
  "description": "redirect with prefix for koa",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha -R spec -t 5000 test/*.js",
    "test-cov": "istanbul cover node_modules/.bin/_mocha -- -R dot -t 5000 test/*.js",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- -R dot -t 5000 test/*.js"
  },
  "repository": "koa-modules/koa-redirects",
  "keywords": [
    "redirects",
    "redirect",
    "prefix",
    "path"
  ],
  "author": "haoxin",
  "license": "MIT",
  "devDependencies": {
    "istanbul-harmony": "0",
    "koa": "0",
    "mocha": "2",
    "supertest": "1"
  }
}
